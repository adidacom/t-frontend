{
  "name": "t4-frontend",
  "version": "0.1.0",
  "homepage": "./",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:dev": "export $(grep -v '^#' .dev.env | xargs) && react-scripts build",
    "build:rincon": "export $(grep -v '^#' .rincon.env | xargs) && react-scripts build",
    "build:production": "export $(grep -v '^#' .production.env | xargs) && react-scripts build",
    "deploy:dev": "export $(grep -v '^#' .dev.env | xargs) && yarn run build:dev && sh ./deploy/deploy.sh",
    "deploy:rincon": "export $(grep -v '^#' .rincon.env | xargs) && yarn run build:rincon && sh ./deploy/deploy.sh",
    "deploy:production": "export $(grep -v '^#' .production.env | xargs) && yarn run build:production && sh ./deploy/deploy.sh",
    "fix-code": "node_modules/.bin/prettier \"./src/**/*.{js,jsx}\" --write",
    "fix-style": "node_modules/.bin/stylelint \"src/**/*.{css,scss}\" --fix",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --trailing-comma all --write",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.7",
    "@fortawesome/free-regular-svg-icons": "^5.7.2",
    "@fortawesome/free-solid-svg-icons": "^5.4.2",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "@material-ui/core": "^3.9.2",
    "@material-ui/styles": "^4.3.0",
    "@sentry/browser": "^5.4.3",
    "@vx/group": "^0.0.190",
    "@vx/shape": "^0.0.190",
    "ag-grid-community": "^20.2.0",
    "ag-grid-react": "^20.2.0",
    "amplitude-js": "^4.6.0-beta.2",
    "antd": "^3.21.4",
    "axios": "^0.19.0",
    "babel-plugin-import": "^1.12.0",
    "bezier-easing": "^2.1.0",
    "classnames": "^2.2.6",
    "d3-hierarchy": "^1.1.8",
    "detect-browser": "^4.0.4",
    "fuse.js": "^3.4.4",
    "grommet": "^2.7.8",
    "grommet-icons": "^4.3.0",
    "history": "^4.9.0",
    "intro.js": "^2.9.3",
    "intro.js-react": "^0.2.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.11",
    "moment": "^2.23.0",
    "nuka-carousel": "^4.5.4",
    "numeral": "^2.0.6",
    "owasp-password-strength-test": "^1.3.0",
    "phone": "^2.3.2",
    "prop-types": "^15.6.2",
    "qs": "^6.6.0",
    "react": "16.9.0",
    "react-animate-height": "^2.0.15",
    "react-app-polyfill": "^1.0.2",
    "react-dom": "^16.6.0",
    "react-ga": "^2.5.6",
    "react-helmet-async": "^1.0.2",
    "react-infinite-scroller": "^1.2.4",
    "react-player": "^1.12.0",
    "react-redux": "6.0.1",
    "react-router-dom": "5.0.0",
    "react-scripts": "2.0.5",
    "react-select": "^2.4.2",
    "react-svg-pan-zoom": "2.18",
    "react-transition-group": "^2.6.0",
    "react-virtualized": "^9.21.1",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.3",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.3.2",
    "styled-theme": "^0.3.3",
    "styled-tools": "^1.7.1",
    "universal-cookie": "^4.0.2",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-knobs": "^5.2.0",
    "@storybook/addon-links": "^5.1.11",
    "@storybook/addon-viewport": "^5.2.0",
    "@storybook/addons": "^5.1.11",
    "@storybook/react": "^5.1.11",
    "babel-loader": "^8.0.6",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "prettier": "^1.14.3",
    "stylelint": "^9.6.0",
    "stylelint-config-airbnb": "^0.0.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^1.0.0",
    "stylelint-scss": "^3.3.2"
  }
}
